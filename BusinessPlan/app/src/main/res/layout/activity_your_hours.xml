                android:text="Sat"
                android:id="@+id/textView8"
                android:layout_column="7"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
